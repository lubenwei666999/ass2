                <form action="/Home/Pay" method="get">
                    <div class="time">
                        Check-in Time:<input id="ChecklnDate" type="date" required name="ChecklnDate" value="@DateTime.Now.ToString("yyyy-MM-dd")">
                        Check-out time:<input id="checkoutDate" type="date" required name="checkoutDate" value="@DateTime.Now.AddDays(1).ToString("yyyy-MM-dd")">
                    </div>
                    @{
                        Random random = new Random();
                    }
                    <div class="flex-js">
                        @foreach (var room in ViewBag.Rooms)
                        {
                            <div class="room">
                                <div class="img">
                                    @if (random.Next(1, 5).ToString() == "1")
                                    {
                                        <img src="~/img/pic3.jpg" width="300" alt="">
                                    }
                                    else if (random.Next(1, 5).ToString() == "2")
                                    {
                                        <img src="~/img/pic4.jpg" width="300" alt="">
                                    }
                                    else if (random.Next(1, 5).ToString() == "3")
                                    {
                                        <img src="~/img/pic7.jpg" width="300" alt="">
                                    }
                                    else
                                    {
                                        <img src="~/img/pic9.jpg" width="300" alt="">
                                    }
                                </div>
